{
	"name": "copy-selection-to",
	"displayName": "Copy/Move Selection To...",
	"description": "Copies or Moves the code selected code into another file just by right clicking on it.",
	"publisher": "cmdhq",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/cybersonic/vscode_extension_copy_to.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.extractText", 
				"title": "Copy selection to..."
			},
			{
				"command": "extension.moveText",
				"title": "Move selection to..."
			}
		],
		"menus":{
			"editor/context": [
				{
					"when":"resourceLangId == cfml",
					"command": "extension.extractText",
					"alt": "extension.extractText",
					"group": "1_modification"
				},
				{
					"when":"resourceLangId == cfml",
					"command": "extension.moveText",
					"alt": "extension.moveText",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"vscode-cache": "^0.3.0"
	}
}
